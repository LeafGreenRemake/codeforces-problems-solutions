#include <bits/stdc++.h>
using namespace std;
 
void solve (int a, int b, int c)
{
	if (a > b)
	{
		if (b > c)
		{
			if (b + a >= 10)
			{
				cout << "YES" << endl;
			}
			else
			{
				cout << "NO" << endl;
			}
		}
		else
		{
			if (a + c >= 10)
			{
				cout << "YES" << endl;
			}
			else
			{
				cout << "NO" << endl;
			}
		}
	}
	else
	{
		if (a > c)
		{
			if (b + a >= 10)
			{
				cout << "YES" << endl;
			}
			else
			{
				cout << "NO" << endl;
			}
		}
		else
		{
			if (b + c >= 10)
			{
				cout << "YES" << endl;
			}
			else
			{
				cout << "NO" << endl;
			}
		}
	}
}
 
int main() {
	int t, a, b, c;
	cin >> t;
	for (int i = 0; i < t; i++)
	{
		cin >> a >> b >> c;
		solve(a, b, c);
	}
}
